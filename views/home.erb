<table>
  <tr>
    <th>Name</th>
    <th>Phone Number</th>
    <th>Email Address</th>
    <th>Category</th>
  </tr>
  <% @contact_table.each do |result| %>
    <tr>
      <td><%= result["name"] %></td>
      <td><%= format_phone(result["phone_number"]) %></td>
      <td><%= result["email"] %></td>
      <td><%= result["category_name"] %></td>
      <td><a href="/edit/<%= result["id"]%>">Edit</a></td>
    </tr>
  <% end %>
</table>

<div class = "new_contact_link">
  <a href="/create"> Create a New Contact</a>
</div>

